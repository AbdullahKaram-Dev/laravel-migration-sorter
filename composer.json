{
  "name": "abdullahkaram-dev/laravel-migration-sorter",
  "description": "Interactive Laravel migration file sorting with drag and drop functionality",
  "type": "library",
  "keywords": ["laravel", "migration", "sorting", "artisan", "command"],
  "homepage": "https://github.com/AbdullahKaram-Dev/laravel-migration-sorter",
  "license": "MIT",
  "authors": [
    {
      "name": "Abdullah Karam",
      "email": "abdallakaramdev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0",
    "nesbot/carbon": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "AbdullahKaramDev\\MigrationSorter\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AbdullahKaramDev\\MigrationSorter\\MigrationSorterServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}